using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        
        int[] nums = {5,8,10,3,99,7,2};
        int i;
        int max = nums[0];
        for(i=0; i < nums.Length; i++)
        {
            if(max < nums[i])
            {
              max = nums[i];
            }
        }
        Console.WriteLine ("The biggest number is: " + max);
    }
     
}        
