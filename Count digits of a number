using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int count = 0;
        int temp;
    Console.WriteLine("Enter a number");
   int a = Convert.ToInt32(Console.ReadLine());
    temp = a;
    while(temp>0){
        temp = temp / 10;
        count++;
        
    }
    Console.WriteLine("The number " + a + " has " +  count  + " digit");
    }
    
}
