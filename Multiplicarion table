using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int n = new int();
        Console.WriteLine("Enter a number");
        int a = Convert.ToInt32(Console.ReadLine());
        for(int i=1;i<=10;i++)
        {
            n = a * i;
            Console.WriteLine(n);
        }
       
    }
}   
